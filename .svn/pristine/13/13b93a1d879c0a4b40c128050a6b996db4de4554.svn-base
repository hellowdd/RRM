    <script type="text/javascript">
    var deg = 0;
    var realwidth;
    var realheight;
    //旋转
    function rotate() {
        deg = parseInt(deg) + 90;
        if (deg > 360) {
            deg = 0;
        }
        if (deg == 90 || deg == 270) {
            var maxwidth = 880;
            var maxheight = 500;
            var ratio = 0;
            var width = $(".fancybox-inner img").width();
            var height = $(".fancybox-inner img").height();
            if (width > maxheight) {
                ratio = maxheight / width;
                $(".fancybox-inner img").css("width", maxheight);
                height = height * ratio;
                $(".fancybox-inner img").css("height", height);
                width = maxheight;
            }
            if (height > maxwidth) {
                ratio = maxwidth / height;
                $(".fancybox-inner img").css("height", maxwidth);
                width = width * ratio;
                $(".fancybox-inner img").css("width", width);
                height = maxwidth;
            }
            height = -parseInt((height / 2));
            $(".fancybox-inner img").css("margin-top", height);
            var width = $(".fancybox-inner img").width();
            width = -parseInt((width / 2));
            $(".fancybox-inner img").css("margin-left", width);
        } else {
            $(".fancybox-inner img").css("height", realheight);
            $(".fancybox-inner img").css("width", realwidth);
            //realheight = -parseInt((realheight / 2));
            $(".fancybox-inner img").css("margin-top", -parseInt((realheight / 2)));
            //realwidth = -parseInt((realwidth / 2));
            $(".fancybox-inner img").css("margin-left", -parseInt((realwidth / 2)));
        }
        $(".fancybox-inner img").css('transform', 'rotate(' + deg + 'deg)');
    }
    //下载
    function downimg(obj) {
        var dataurl = $(obj).attr('data-url');
        var datasize = $("select[name=size]").val();
        datasize = datasize.split('*');
        var datawidth = datasize[0];
        var dataheight = datasize[datasize.length - 1];
        var datatype = $("select[name=type]").val();
        datatype = datatype.split('.');
        datatype = datatype[datatype.length - 1];
        var dataname = $(obj).attr('data-name');
        console.log(datawidth);
        console.log(dataheight);
        //console.log(datatype);
        window.location.href = '/RRM/manager/img/download?url=' + dataurl + '&type=' + datatype + '&width=' + datawidth + '&height=' + dataheight + '&name=' + dataname + '';


    }

    $('.test').fancybox({
        arrows: false,

        closeBtn: true,

        helpers: {

            title: {

                type: 'outside'

            }


        },
        afterLoad: function() {
            deg = 0;
            this.title = '<p class="af-down fleft"><a href="javascript:void(0)" onclick="downimg(this)" data-url="' + this.href + '" data-name="' + this.title + '"><i class="fa fa-download" aria-hidden="true"></i>下载</a></p><p class="af-rotate fleft" onclick="rotate()"><i class="fa fa-repeat" aria-hidden="true"></i>旋转</p><p class="af-title fleft">' + this.title + '</p><p class="fright af-size"></p><div class="fright af-fr af-kind" style="position:relative">格式:<select name="type" style="position:absolute"><option value="jpg">.jpg</option><option value="bmp">.bmp</option><option value="png">.png</option><option value="gif">.gif</option><option value="jpeg">.jpeg</option></select><i class="fa fa-chevron-down fm" aria-hidden="true"></i></div><div class="fright af-fr af-size" style="position:relative">分辨率:<select style="position:absolute" name="size"><option value="1280*960">1280*960</option><option value="1024*768">1024*768</option><option value="800*600">800*600</option><option value="640*480">640*480</option></select><i class="fa fa-chevron-down" aria-hidden="true"></i></div>';

        },
        afterShow: function() {
            var fitype = $(".fancybox-inner img").attr("src");
            fitype = fitype.split('.')
            fitype = fitype[fitype.length - 1];
            fitype = fitype.toLowerCase();
            var flag = true;
            var ftag = true;
            var maxwidth = 880;
            var maxheight = 500;
            var ratio = 0;
            var width = $(".fancybox-inner img").width();
            var height = $(".fancybox-inner img").height();
            var imgsize = width + '*' + height;
            var oplen = $("select[name=size] option").length;
            for (var i = 0; i < oplen; i++) {
                if ($("select[name=size] option").eq(i).val() == imgsize) {
                    $("select[name=size]").val(imgsize);
                    break;
                } else {
                    flag = false;
                }
            }
            if (flag == false) {
                $("select[name=size]").append('<option value="' + imgsize + '">' + imgsize + '</option>');
                $("select[name=size]").val(imgsize);
            }
            //格式
            var tylen = $("select[name=type] option").length;
            for (var i = 0; i < tylen; i++) {
                // console.log($("select[name=type] option").eq(i).val());
                if ($("select[name=type] option").eq(i).val() == fitype) {
                    $("select[name=type]").val(fitype);
                    break;
                }
            }

            if (width > maxwidth) {
                ratio = maxwidth / width;
                $(".fancybox-inner img").css("width", maxwidth);
                height = height * ratio;
                $(".fancybox-inner img").css("height", height);
                width = maxwidth;
            }
            if (height > maxheight) {
                ratio = maxheight / height;
                $(".fancybox-inner img").css("height", maxheight);
                width = width * ratio;
                $(".fancybox-inner img").css("width", width);
                height = maxheight;
            }
            realwidth = width;
            realheight = height;
            height = -parseInt((height / 2));
            $(".fancybox-inner img").css("margin-top", height);
            var width = $(".fancybox-inner img").width();
            width = -parseInt((width / 2));
            $(".fancybox-inner img").css("margin-left", width);

        }

    });
    </script>
