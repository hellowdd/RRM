body {
    font-family: 'Microsoft YaHei' !important;
}

.service {
    cursor: pointer;
}



/* 退出登录 */

.userupload {
    display: block;
    float: left;
    width: 18px;
    height: 16px;
    margin-right: 20px;
    margin-top: 24px;
    cursor: pointer;
}

.hed-wid {
    position: relative;
}

.bod {
    min-height: 800px;
}

input::-ms-clear {
    display: none;
}

select::-ms-clear {
    display: none;
}

select::-ms-expand {
    display: none;
}

.exit {
    width: 100px;
    background: #fff;
    color: #999;
    position: absolute;
    top: 65px;
    box-shadow: 0 0 13px 0 rgba(0, 0, 0, 0.2);
    right: -63px;
    cursor: pointer;
}

.exit li {
    line-height: 40px;
    text-align: center;
    font-size: 15px;
    letter-spacing: 1px;
    text-align: center;
}

.exit li:hover {
    color: #666;
}

.th-list {
    cursor: pointer;
}

.exit:before {
    position: absolute;
    display: inline-block;
    top: -8px;
    left: 16px;
    width: 0;
    height: 0px;
    content: '';
    border-style: solid;
    border-width: 10px;
    border-color: #fff #fff transparent transparent;
    transform: rotate(-45deg);
    box-shadow: 2px -1px 8px rgba(0, 0, 0, 0.1);
}



/* 必填项 */

b {
    color: red;
}

.up-label-box p {
    position: relative;
}

.up-label-box p i {
    position: absolute;
    right: -5px;
    top: -7px;
    color: #1996e7;
    cursor: pointer;
}



/* 导航栏 */

.retrie:after {
    content: '';
    width: 34px;
    height: 4px;
    display: block;
    position: absolute;
    bottom: -10px;
    background: #1996e7;
    left: 2px;
}

.retrie-span:after {
    content: '';
    width: 34px;
    height: 4px;
    display: none;
    position: absolute;
    bottom: -10px;
    background: #1996e7;
    left: 2px;
}

.wid {
    height: 100%;
    width: 1200px;
    margin: 0 auto;
}

.imgbox {
    overflow: hidden;
    clear: both;
}

.img-item {
    width: 18%;
    float: left;
    text-align: center;
    box-shadow: 0 0 9px 0px rgba(0, 0, 0, 0.28);
    margin-left: 1%;
    margin-right: 1%;
    margin-bottom: 35px;
}

.img-text {
    width: 100%;
    background: #fff;
    text-align: center;
    color: #999;
    line-height: 38px;
    padding: 0px 10px;
    height: 38px;
    font-size: 1.2rem;
    font-size: 12px;
}

.img-text a {
    display: block;
    font-size: 1.2rem;
    color: #999;
}

.img-text a i {
    margin-right: 6px;
}

.img-item-box {
    width: 100%;
    height: 216px;
    overflow: hidden;
}

.img-item-box img {
    width: 100%;
    min-height: 216px;
}

.all-down {
    font-size: 1.6rem;
    font-size: 16px;
    color: #999;
    display: block;
    margin-bottom: 15px;
    margin-left: 1%;
    cursor: pointer;
}





/* 导航栏 */

.hedcon {
    box-shadow: 0 0 20px 2px rgba(0, 0, 0, 0.2);
    height: 70px;
}

.wid-hed {
    width: 100%;
    height: 70px;
    font-size: 18px;
    font-size: 1.8rem;
    color: #b3b3b3;
}

.wid-hed p {
    line-height: 62px;
}

.logo {
    width: 105px;
    height: 23px;
    margin-right: 90px;
}

.retrie {
    margin-right: 75px;
    position: relative;
    border-bottom: 3px;
    cursor: pointer;
}

.retrie i {
    margin-left: 10px;
}

.retrie-ul {
    position: absolute;
    top: 71px;
    background: #fff;
    width: 140px;
    text-align: center;
    color: #b3b3b3;
    font-size: 1.8rem;
    box-shadow: 3px 1px 9px 0 rgba(0, 0, 0, 0.1);
    padding-top: 10px;
    padding-bottom: 10px;
    line-height: 40px;
    left: -38px;
    z-index: 999;
}

.retrie-ul:after {
    position: absolute;
    display: inline-block;
    top: -8px;
    left: 79px;
    width: 0;
    height: 0px;
    content: '';
    border-style: solid;
    border-width: 10px;
    border-color: #fff #fff transparent transparent;
    transform: rotate(-45deg);
    box-shadow: 2px -1px 8px rgba(0, 0, 0, 0.1);
}

.retrie-ul li a {
    color: #b3b3b3;
}

.retrie-ul li a:hover {
    color: #666;
}

.hed-left img {
    margin-top: 20px;
    padding-left: 10px;
}

.hed-right p {
    margin-right: 30px;
}

.userlogo {
    width: 35px;
    height: 36px;
    overflow: hidden;
    margin-top: 15px;
    margin-right: 10px;
}

.userlogo img {
    width: 100%;
    min-height: 36px;
    display: block;
}

.disnone {
    display: none;
}



/* 搜索框部分 */

.bod {
    background: #f8faff;
    margin-top: 10px;
    padding-top: 65px;
}

.search-box {
    width: 825px;
    height: 54px;
    background: #fff;
    box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.2);
    margin: 0 auto;
    border-radius: 4px;
    border-bottom-left-radius: 0px;
}

.sea-sele {
    position: relative;
    width: 125px;
    text-align: center;
    float: left;
    padding-right: 10px;
    cursor: pointer;
}

.sea-sele:after {
    content: '';
    width: 1px;
    height: 34px;
    background: #e4e9f4;
    position: absolute;
    top: 10px;
    right: 0px;
}

.sea-sele p {
    font-size: 1.6rem;
    color: #999999;
    line-height: 54px;
}

.sea-sele p i {
    margin-left: 15px;
    position: absolute;
    right: 15px;
    top: 20px;
}

.sea-option {
    width: 100%;
    background: #fff;
    position: absolute;
    top: 54px;
    box-shadow: 1px 7px 12px 0px rgba(0, 0, 0, 0.2);
    padding-right: 10px;
    cursor: pointer;
    z-index: 999;
}

.sea-option-item {
    font-size: 1.6rem;
    color: #999;
    line-height: 40px;
}

.sea-option-item:hover {
    color: #666;
}

.se-icon {
    float: right;
    margin-right: 23px;
    color: #b3b3b3;
    margin-top: 15px;
    cursor: pointer;
    position: relative;
}

.se-icon i {
    font-size: 2.4rem;
    margin-right: 6px;
}

.se-icon:after {
    content: '';
    width: 1px;
    height: 24px;
    background: #b3b3b3;
    position: absolute;
    top: 2px;
    right: -10px;
    opacity: 0.5;
}

#search {
    width: 600px;
    background: none;
    outline: none;
    border: none;
    line-height: 54px;
    height: 54px;
    text-indent: 20px;
    font-size: 1.6rem;
    color: #999;
}

.sea-filter {
    float: right;
    display: block;
    margin-top: 17px;
    margin-right: 15px;
    cursor: pointer;
}